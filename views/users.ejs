<html>
	<head>
		<link rel="stylessheet" href="/lib/bootstrap/dist/css/bootstrap.min.css">
		<link rel="stylessheet" href="/lib/font-awesome/css/font-awesome.min.css">
		<link rel="stylessheet" href="/stylesheets/style.css">

		 <script src="/lib/jquery/dist/jquery.js"></script>
		 <script src="/lib/bootstrap/dist/js/bootstrap.js"></script>
	</head>

	<body>
	<div>
		<img src="<%=user.image%>">
		<%=user.displayName%>
		<%if(user.facebook){%>
			<img src="<%=user.facebook.image%>">
			Friends count: <%=user.facebook.friends%> 
		<%}%>
	</div>
	<div>
		<ul>
			<li>
				<a href="/auth/google" class="googleplus"> Google
					<%if(user.google){%> -> You have google plus sign in <%}%>
				</a>
			</li>
			<li>
				<a href="/auth/twitter" class="twitter">
					<i class="fa fa-twitter" aria-hidden="true"></i>Twitter
					<%if(user.twitter){%> ->  You have twitter sign in <%}%>
				</a>
			</li>
			<li>
				<a href="/auth/facebook" class="facebook">
					<i class="fa fa-twitter" aria-hidden="true"></i>Facebook 
					<%if(user.facebook){%> ->  You have facebook sign in <%}%>
				</a>
			</li>
		</ul>
	</div>
	</body>
</html>
